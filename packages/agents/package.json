    "test:ci": "jest --coverage --ci",
    "verify": "npm run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanzoai/chat.git"
  },
  "author": "Hanzo Industries Inc <dev@hanzo.ai>",
